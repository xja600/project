<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.dao.mapper.UserInfoMapper">

    <select id="getUserInfo" resultType="com.spring.dao.entity.UserInfo" >
      select * from user_info where userid=#{userInfo.userid}
    </select>

</mapper>
